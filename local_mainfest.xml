<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android">

	<!-- Remote -->
	<remote
		name="kdrag0n"
		fetch="https://github.com/kdrag0n"/>

	<remote
		name="c3eru"
		fetch="https://github.com/c3eru"/>

	<remote
		name="MOBX-PROJECTS"
		fetch="https://github.com/MOBX-PROJECTS"/>

	<remote
		name="greenforce-project"
		fetch="https://github.com/greenforce-project"/>

	<remote 
		name="pixel-devices"
		fetch="https://github.com/PixelExperience-Devices"
		revision="thirteen" />

	<remote
		name="pixel-devices-blobs"
		fetch="https://gitlab.pixelexperience.org/android/vendor-blobs"
		revision="thirteen" />

	<remote
		name="xd"
		fetch="https://github.com/xdroid-oss"
		revision="thirteen"/>

	<!-- DT/VT/KT/TC -->
	<project
		path="device/xiaomi/chime"
		name="device_xiaomi_chime"
		remote="MOBX-PROJECTS"
		revision="thirteens"
		clone-depth="1"/>

	<project
		path="vendor/xiaomi/chime"
		name="vendor_xiaomi_chime"
		remote="pixel-devices-blobs"
		clone-depth="1"/>

	<project
		path="kernel/xiaomi/chime"
		name="kernel_xiaomi_chime"
		remote="pixel-devices"
		revision="thirteen"
		clone-depth="1"/>
		
	<project
		path="prebuilts/clang/host/linux-x86/clang-proton"
		name="proton-clang"
		remote="kdrag0n"
		revision="master"
		clone-depth="1"/>
		
	<!-- Replacement -->
	<remove-project
		name="android_hardware_lineage_compat"/>

	<project
		path="hardware/xiaomi"
		name="xd_hardware_xiaomi"
		remote="xd"/>

</manifest>

