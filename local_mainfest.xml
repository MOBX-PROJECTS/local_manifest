<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android">

	<!-- Remote -->
	<remote
		name="kdrag0n"
		fetch="https://github.com/kdrag0n"/>

	<remote
		name="c3eru"
		fetch="https://github.com/c3eru"/>

	<remote
		name="MOBX-PROJECTS"
		fetch="https://github.com/MOBX-PROJECTS"/>

	<remote
		name="greenforce-project"
		fetch="https://github.com/greenforce-project"/>

	<!-- DT/VT/KT/TC -->
	<project
		path="device/xiaomi/chime"
		name="device_xiaomi_chime"
		remote="MOBX-PROJECTS"
		revision="thirteen"
		clone-depth="1"/>

	<project
		path="vendor/xiaomi/chime"
		name="vendor_xiaomi_chime"
		remote="pixel-devices-blobs"
		clone-depth="1"/>

	<project
		path="kernel/xiaomi/chime"
		name="kernel_xiaomi_citrus_sm6115"
		remote="greenforce-project"
		revision="main"
		clone-depth="1"/>
		
	<project
		path="prebuilts/clang/host/linux-x86/clang-proton"
		name="proton-clang"
		remote="kdrag0n"
		revision="master"
		clone-depth="1"/>
		
	<!-- HALS -->
	<remove-project
		name="hardware_qcom-caf_sm8250_audio"/>

	<remove-project
		name="hardware_qcom-caf_sm8250_display"/>

	<remove-project
		name="hardware_qcom-caf_sm8250_media"/>

	<project
		path="hardware/qcom-caf/sm8250/audio"
		name="android_hardware_qcom_audio"
		remote="MOBX-PROJECTS"
		revision="thirteen"
		clone-depth="1"/>

	<project
		path="hardware/qcom-caf/sm8250/display"
		name="android_hardware_qcom_display"
		remote="MOBX-PROJECTS"
		revision="thirteen"
		clone-depth="1"/>

	<project
		path="hardware/qcom-caf/sm8250/media"
		name="android_hardware_qcom_media"
		remote="MOBX-PROJECTS"
		revision="thirteen"
		clone-depth="1"/>

	<!-- Replacement -->
	<remove-project
		name="vendor_pixel-framework"/>

	<project
		path="vendor/pixel-framework"
		name="vendor_pixel-framework"
		remote="c3eru"
		revision="staging"/>

	<remove-project
		name="frameworks_base"/>

	<project
		path="frameworks/base"
		name="fwb_aosp"
		groups="pdk-cw-fs,pdk-fs"
		remote="c3eru"
		revision="staging"
		clone-depth="1"/>

	<remove-project
		name="packages_apps_Settings"/>

	<project
		path="packages/apps/Settings"
		name="Settings"
		remote="c3eru"
		revision="staging"/>

</manifest>

